<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.limon.http.dao.FeedBackDao">
	<insert id="saveFeedBack" parameterType="java.util.Map" useGeneratedKeys="true" keyProperty="id">
		insert into app_feedback(userid,content,createtime)values(#{userid},#{content},#{createtime})
    </insert>
</mapper>
