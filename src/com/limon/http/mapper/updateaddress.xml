<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.limon.http.dao.UpdateAddressDao">
	<update id="updateUserAddress" parameterType="java.util.Map">
		update app_useraddr set province=#{province},city=#{city},county=#{county},address=#{address},contact=#{contact},mobile=#{mobile} where id=#{id}
    </update>
</mapper>
